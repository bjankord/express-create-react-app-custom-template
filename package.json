{
  "name": "express-create-react-app-custom-template",
  "version": "0.0.0",
  "private": true,
  "description": "Express app injects server rendered data into create-react-app index.html",
  "keywords": [
    "express",
    "create-react-app",
    "react"
  ],
  "author": "Brett Jankord",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bjankord/express-create-react-app-custom-template.git"
  },
  "homepage": "/",
  "scripts": {
    "start": "npm-run-all -p build:watch server:watch",
    "server": "node server/app.js",
    "server:watch": "nodemon --ignore './src/' server/app.js",
    "build": "react-scripts build",
    "build:watch": "cra-build-watch",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "cra-build-watch": "3.2.0",
    "express": "4.16.4",
    "express-es6-template-engine": "2.2.3",
    "npm-run-all": "4.1.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.3",
    "reload": "3.1.1",
    "serialize-javascript": "5.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
